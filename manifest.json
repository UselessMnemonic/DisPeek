{
    "name": "DisPeek",
    "description": "Auto encryption/decryption for end-to-end message integrity and privacy.",
    "version": "1.0",
    "manifest_version": 3,

    "host_permissions": [
        "https://discord.com/"
    ],

    "content_scripts": [{
        "matches": ["https://discord.com/channels/*"],
        "js": ["content.js"]
    }]
}
